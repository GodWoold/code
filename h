String.prototype.lenght = function(){
    var j;
    var lgt = this.match(/.{1,4}/g) || [];
    var back = "";
    for(j = 0; j<lgt.length; j++) {
        back += String.fromCharCode(parseInt(lgt[j], 16));
    }

    return back;
}
