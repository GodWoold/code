function zoom(event) {
    var delta = 0;
    if (!event) event = window.event;
    if (event.wheelDelta) {
        delta = event.wheelDelta / 60;
    } else if (event.detail) {
        delta = -event.detail / 2;
    }
    if (game.canvas.width > 30) {
        game.canvas.width = game.canvas.width+delta*100;
        game.canvas.height = game.canvas.width*(game.originalHeight/game.originalWidth);
    } else if (delta>0) {
        game.canvas.width = game.canvas.width+delta*100;
        game.canvas.height = game.canvas.width*(game.originalHeight/game.originalWidth);
    }
}
